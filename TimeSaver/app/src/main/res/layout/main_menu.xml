<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/purple">

    <TextView
        android:id="@+id/menuText"
        android:layout_width="135dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/comfortaa_bold"
        android:padding="16dp"
        android:text="Menu"
        android:textColor="@color/black"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.097" />

    <TextView
        android:id="@+id/categoryText"
        android:layout_width="189dp"
        android:layout_height="184dp"
        android:background="@drawable/black_block"
        android:fontFamily="@font/comfortaa_bold"
        android:gravity="center"
        android:paddingBottom="50dp"
        android:text="Create Category"
        android:textColor="@color/purple"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.268" />

    <TextView
        android:id="@+id/timesheetTextview"
        android:layout_width="189dp"
        android:layout_height="184dp"
        android:background="@drawable/black_block"
        android:fontFamily="@font/comfortaa_bold"
        android:gravity="center"
        android:paddingBottom="50dp"
        android:text="Create Timesheet"
        android:textColor="@color/purple"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.972"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.268" />

    <TextView
        android:id="@+id/Notifications"
        android:layout_width="189dp"
        android:layout_height="184dp"
        android:background="@drawable/black_block"
        android:fontFamily="@font/comfortaa_bold"
        android:gravity="center"
        android:paddingBottom="50dp"
        android:text="Notifications"
        android:textColor="@color/purple"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.97" />

    <TextView
        android:id="@+id/contact_textView"
        android:layout_width="189dp"
        android:layout_height="184dp"
        android:background="@drawable/black_block"
        android:fontFamily="@font/comfortaa_bold"
        android:gravity="center"
        android:paddingBottom="50dp"
        android:text="Contact Us"
        android:textColor="@color/purple"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.972"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.97" />

    <TextView
        android:id="@+id/view"
        android:layout_width="189dp"
        android:layout_height="184dp"
        android:background="@drawable/black_block"
        android:fontFamily="@font/comfortaa_bold"
        android:gravity="center"
        android:paddingBottom="50dp"
        android:text="View"
        android:textColor="@color/purple"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.972"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.619" />

    <TextView
        android:id="@+id/setDailyGoal"
        android:layout_width="189dp"
        android:layout_height="184dp"
        android:background="@drawable/black_block"
        android:fontFamily="@font/comfortaa_bold"
        android:gravity="center"
        android:paddingBottom="50dp"
        android:text="Set Daily  Goal"
        android:textColor="@color/purple"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.619" />


    <TextView
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/categoryText"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text="View timesheets, daily goals, total hours, archived projects and achievements"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.889"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.665" />

    <TextView
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/categoryText"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text="Set a minimum and maximum daily goal for hours worked"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.138"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.667" />

    <TextView
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/contact_textView"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text="Create a category for timesheet entries to belong to"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.138"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.389" />

    <TextView
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/contact_textView"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text="Create a timesheet entry"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.889"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.389" />

    <TextView
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/contact_textView"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text="View notifications"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.138"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.889" />

    <TextView
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/contact_textView"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:text="Send us a message"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.886"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.889" />

    <ImageView
        android:id="@+id/back_button"
        android:layout_width="59dp"
        android:layout_height="31dp"
        android:src="@drawable/back_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.017"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.092" />
</androidx.constraintlayout.widget.ConstraintLayout>

